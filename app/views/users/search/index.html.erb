<h1>Users</h1>

<%= form_with url: search_users_path do |form| %>
<%= form.text_field 'search_term', placeholder: 'enter search terms...' %>
<%= form.submit 'Search', data: { disable_with: false } %>
<% end %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"><th>
    </tr>
  </thead>

  <%= render partial: "users/listing" %>
</table>
